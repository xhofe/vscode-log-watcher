{
  "publisher": "xhofe",
  "name": "vscode-log-watcher",
  "displayName": "vscode-log-watcher",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/xhofe",
  "homepage": "https://github.com/xhofe/vscode-log-watcher#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/xhofe/vscode-log-watcher"
  },
  "bugs": {
    "url": "https://github.com/xhofe/vscode-log-watcher/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/xhofe"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-log-watcher.selectLogFile",
        "title": "选择/切换文件",
        "category": "Log Watcher"
      },
      {
        "command": "vscode-log-watcher.setLogLevelFilter",
        "title": "日志等级过滤",
        "category": "Log Watcher"
      },
      {
        "command": "vscode-log-watcher.setKeywordFilter",
        "title": "关键字过滤",
        "category": "Log Watcher"
      },
      {
        "command": "vscode-log-watcher.setHighlightKeyword",
        "title": "关键字高亮",
        "category": "Log Watcher"
      },
      {
        "command": "vscode-log-watcher.pause",
        "title": "暂停监听",
        "category": "Log Watcher"
      },
      {
        "command": "vscode-log-watcher.resume",
        "title": "恢复监听",
        "category": "Log Watcher"
      },
      {
        "command": "vscode-log-watcher.clearEntries",
        "title": "清空日志",
        "category": "Log Watcher"
      },
      {
        "command": "vscode-log-watcher.toggleAutoScroll",
        "title": "自动滚动",
        "category": "Log Watcher"
      },
      {
        "command": "vscode-log-watcher.formatJsonLine",
        "title": "JSON 格式化选中行",
        "category": "Log Watcher"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "vscode-log-watcher",
          "title": "Log Watcher",
          "icon": "res/icon.png"
        }
      ]
    },
    "views": {
      "vscode-log-watcher": [
        {
          "id": "vscode-log-watcher.logPanel",
          "name": "Log Watcher"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "vscode-log-watcher.selectLogFile",
          "when": "view == vscode-log-watcher.logPanel",
          "group": "navigation@0"
        },
        {
          "command": "vscode-log-watcher.setLogLevelFilter",
          "when": "view == vscode-log-watcher.logPanel",
          "group": "navigation@10"
        },
        {
          "command": "vscode-log-watcher.setKeywordFilter",
          "when": "view == vscode-log-watcher.logPanel",
          "group": "navigation@20"
        },
        {
          "command": "vscode-log-watcher.setHighlightKeyword",
          "when": "view == vscode-log-watcher.logPanel",
          "group": "navigation@30"
        },
        {
          "command": "vscode-log-watcher.pause",
          "when": "view == vscode-log-watcher.logPanel && !vscode-log-watcher.paused",
          "group": "navigation@40"
        },
        {
          "command": "vscode-log-watcher.resume",
          "when": "view == vscode-log-watcher.logPanel && vscode-log-watcher.paused",
          "group": "navigation@40"
        },
        {
          "command": "vscode-log-watcher.clearEntries",
          "when": "view == vscode-log-watcher.logPanel",
          "group": "navigation@50"
        },
        {
          "command": "vscode-log-watcher.toggleAutoScroll",
          "when": "view == vscode-log-watcher.logPanel",
          "group": "navigation@60"
        }
      ],
      "view/item/context": [
        {
          "command": "vscode-log-watcher.formatJsonLine",
          "when": "view == vscode-log-watcher.logPanel && viewItem == logLine",
          "group": "inline"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "vscode-log-watcher",
      "properties": {
        "vscode-log-watcher.defaultFile": {
          "type": "string",
          "default": "",
          "markdownDescription": "扩展激活时自动监听的默认日志文件绝对路径。留空则不自动打开。"
        },
        "vscode-log-watcher.contentTransform": {
          "type": "string",
          "default": "",
          "markdownDescription": "自定义 JavaScript 代码，用于将每一行日志转换成面板展示内容。可写函数表达式（例如 `line => line.trim()`）或函数体（例如 `return line.trim()`）。当返回 `undefined` 或抛出异常时，将回退为原始日志行。"
        }
      }
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --external vscode",
    "dev": "nr build --watch --sourcemap",
    "prepare": "nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "nr build",
    "release": "bumpp",
    "ext:package": "vsce package --no-dependencies",
    "ext:publish": "vsxpub --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@antfu/ni": "^23.3.1",
    "@types/node": "^22.13.4",
    "@types/vscode": "^1.97.0",
    "@vscode/vsce": "^3.2.2",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "ovsx": "^0.10.5",
    "pnpm": "^10.4.1",
    "reactive-vscode": "^0.2.10",
    "taze": "^19.8.1",
    "tsdown": "^0.9.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "vscode-ext-gen": "^1.0.0",
    "vsxpub": "^0.1.2"
  }
}
